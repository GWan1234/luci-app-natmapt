{
	"luci-app-natmap": {
		"description": "Grant access to LuCI app natmap",
		"read": {
			"file": {
				"/var/run/natmap/*": [ "read" ],
				"/tmp/natmap-natBehavior": [ "read" ],
				"/usr/libexec/natmap/natcheck.sh": [ "exec" ]
			},
			"ubus": {
				"service": [ "list" ]
			},
			"uci": ["firewall", "natmap"]
		},
		"write": {
			"uci": ["firewall", "natmap"]
		}
	}
}
